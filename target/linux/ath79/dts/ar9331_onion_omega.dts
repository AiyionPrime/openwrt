/dts-v1/;

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

#include "ar9331.dtsi"

/ {
	model = "Onion Omega";
	compatible = "onion,omega", "qca,ar9331";

	aliases {
		serial0 = &uart;
		led-boot = &led_system;
		led-failsafe = &led_system;
		led-running = &led_system;
		led-upgrade = &led_system;
	};

	leds {
		compatible = "gpio-leds";

		expansion-dock-blue {
			label = "exp-dock-rgb:blue:expansion-dock-blue";
			gpios = <&gpio 15 GPIO_ACTIVE_LOW>;
		};
		expansion-dock-green {
			label = "exp-dock-rgb:green:expansion-dock-green";
			gpios = <&gpio 16 GPIO_ACTIVE_LOW>;
		};
		expansion-dock-red {
			label = "exp-dock-rgb:red:expansion-dock-red";
			gpios = <&gpio 17 GPIO_ACTIVE_LOW>;
		};

		led_system: system {
			label = "onion:amber:system";
			gpios = <&gpio 27 GPIO_ACTIVE_LOW>;
		};
	};

	keys {
		compatible = "gpio-keys-polled";
		poll-interval = <100>;

		button0 {
			label = "reset";
			linux,code = <KEY_RESTART>;
			gpios = <&gpio 11 GPIO_ACTIVE_HIGH>;
		};
	};
};

&ref {
	clock-frequency = <25000000>;
};

&uart {
	status = "okay";
};

&gpio {
	status = "okay";
};

&usb {
	dr_mode = "host";
	status = "okay";
};

&usb_phy {
	status = "okay";
};

&eth0 {
	status = "okay";
	compatible = "syscon", "simple-mfd";
};

&eth1 {
	status = "okay";
	mtd-mac-address = <&uboot 0x01fc00>;
	mac-address-increment = <(-1)>;

	gmac-config {
		device = <&gmac>;
		switch-phy-addr-swap = <4>;
		switch-phy-swap = <4>;
	};
};


&spi {
	num-chipselects = <1>;
	status = "okay";

	/* Winbond 25Q128FVSG SPI flash */
	flash@0 {
		compatible = "winbond,w25q128", "jedec,spi-nor";
		spi-max-frequency = <20000000>;
		reg = <0>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			uboot: partition@0 {
				label = "u-boot";
				reg = <0x000000 0x020000>;
				read-only;
			};

			partition@20000 {
				compatible = "tplink,firmware";
				label = "firmware";
				reg = <0x020000 0xfd0000>;
			};

			art: partition@ff0000 {
				label = "art";
				reg = <0xff0000 0x010000>;
				read-only;
			};
		};
	};
};

&wmac {
	status = "okay";
	mtd-cal-data = <&art 0x1000>;
	mtd-mac-address = <&uboot 0x01fc00>;
};
